  apiVersion: apps/v1 
  kind: Deployment 
  metadata: 
    name: myapp-deployment 
  spec: 
    replicas: 2 
    selector: 
      matchLabels: 
        app: myapp 
    template: 
      metadata: 
        labels: 
          app: myapp 
      spec: 
        containers: 
        - name: myapp 
          image: myapp:1.0 
          ports: 
          - containerPort: 8080